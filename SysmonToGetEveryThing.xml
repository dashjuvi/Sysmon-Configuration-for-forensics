				1::::1   
				1::::l   
				1::::l   
				1::::l   
				1::::l   
				1::::l   
			 111::::::111
			 1::::::::::1
			 1::::::::::1
			 111111111111         
  -->
    <ProcessCreate onmatch="exclude">
          </ProcessCreate>
	
	<!--Event 2: File Creation time
                             
		  222222222222222    
		2:::::::::::::::22  
	    2:::::222222:::::2 
		2222222    2:::::2 
				   2:::::2 
				   2:::::2 
				2222::::2  
			22222::::::22   
		  22::::::::222     
		2:::::22222        
		2:::::2             
		2:::::2             
		2:::::2       222222
		2::::::2222222:::::2
		2::::::::::::::::::2
		22222222222222222222                
            
  -->
    <FileCreateTime onmatch="exclude">
          </FileCreateTime>
	<!--Event 3: Network Connection                  
                   
		333333333333333   
		3:::::::::::::::33 
		3::::::33333::::::3
		3333333     3:::::3
					3:::::3
					3:::::3
			33333333:::::3 
			3:::::::::::3  
			33333333:::::3 
					3:::::3
					3:::::3
					3:::::3
		3333333     3:::::3
		3::::::33333::::::3
		3:::::::::::::::33 
		333333333333333        
  -->
	
    <NetworkConnect onmatch="exclude">
          </NetworkConnect>
	
<!--Event 5: Process Terminated
        
		555555555555555555 
		5::::::::::::::::5 
		5::::::::::::::::5 
		5:::::555555555555 
		5:::::5            
		5:::::5            
		5:::::5555555555   
		5:::::::::::::::5  
		555555555555:::::5 
					5:::::5
					5:::::5
		5555555     5:::::5
		5::::::55555::::::5
		55:::::::::::::55 
		55:::::::::55   
			555555555                   
   -->                
                
    <ProcessTerminate onmatch="exclude">
          </ProcessTerminate>
	
	<!--Event 6: Driver loaded
	                
				66666666   
			   6::::::6    
			  6::::::6     
			 6::::::6      
			6::::::6       
		   6::::::6        
		  6::::::6         
		 6::::::::66666    
		 6::::::::::::::66  
		 6::::::66666:::::6 
		 6:::::6     6:::::6
		 6:::::6     6:::::6
		 6::::::66666::::::6
		  66:::::::::::::66 
		   66:::::::::66   
			 666666666     

	-->

    <DriverLoad onmatch="exclude" />
        </DriverLoad>
	
		<!--Event 7: DLLs loaded
          
		77777777777777777777
		7::::::::::::::::::7
		7::::::::::::::::::7
		777777777777:::::::7
					7::::::7 
				   7::::::7  
				  7::::::7   
			     7::::::7    
				7::::::7     
			   7::::::7      
			  7::::::7       
			 7::::::7        
			7::::::7         
		   7::::::7          
		  7::::::7           
		 77777777                        
                    
		-->

    <ImageLoad onmatch="exclude" />
	
	<!--Event 8: Remote Thread Created
	                                  
			888888888     
		  88:::::::::88   
		88:::::::::::::88 
		8::::::88888::::::8
		8:::::8     8:::::8
		8:::::8     8:::::8
		 8:::::88888:::::8 
		  8:::::::::::::8  
		 8:::::88888:::::8 
		8:::::8     8:::::8
		8:::::8     8:::::8
